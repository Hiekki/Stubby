{
    "name": "stubby",
    "version": "1.1.0",
    "description": "Stubby, for all things tickets",
    "main": "index.js",
    "scripts": {
        "start": "pm2 start config.json && pm2 logs",
        "restart": "pm2 restart config.json && pm2 logs",
        "dev": "bun run --watch src/Bot.ts",
        "build": "shx rm -rf dist && tsc",
        "format": "prettier --write \"**/*.ts\"",
        "setup": "npm i",
        "pull": "git pull && npm run build && npm start",
        "generate": "git pull && npx prisma generate && npm run build && npm start"
    },
    "keywords": [],
    "author": "Hiekki",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@prisma/client": "^5.18.0",
        "athena": "^2.4.7",
        "dotenv": "^16.4.5",
        "ioredis": "^5.4.1",
        "logger": "^1.0.6",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "numeral": "^2.0.6",
        "simple-slash": "^1.2.0"
    },
    "devDependencies": {
        "@types/node": "^22.3.0",
        "@types/numeral": "^2.0.5",
        "@types/ws": "^8.5.12",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "prisma": "^5.18.0",
        "shx": "^0.3.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.4"
    }
}
